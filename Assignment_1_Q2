package com.company;

public class Main {

    public static void main(String[] args) {
        int score = 10;

        if (score >= 300) {
            System.out.println(" Prefect..!");

        } else if (score > 200) {
            System.out.println(" Very Good..!");

        } else if (score >= 50 && score <= 200) {
            System.out.println(" Good..!");

        } else if (score < 50)
            System.out.println(" Try Again..!");
        }
    }
